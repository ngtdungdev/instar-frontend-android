<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/ic_instagram_icon"
        android:label="Instar"
        android:roundIcon="@drawable/ic_instagram_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.Instarfrontendandroid"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.activities.LoginOtherActivity"/>
        <activity android:name=".ui.activities.LoadingScreenActivity"/>
        <activity android:name=".ui.activities.LoginEmailActivity"/>
        <activity android:name=".ui.activities.LoginActivity"/>
        <activity android:name=".ui.activities.LoginSmsActivity"/>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
        <activity android:name=".ui.activities.HomeActivity"/>
    </application>

</manifest>